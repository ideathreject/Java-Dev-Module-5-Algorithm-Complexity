@startuml

start

:Check if n <= 1;

if (n <= 1?) then (yes)
  :Return n;
  stop
else (no)
  :Check if memo contains n;

  if (memoized?) then (yes)
    :Return memo[n];
    stop
  else (no)
    :Call calculate(n - 1);
    :Call calculate(n - 2);
    :Add results;
    :Store result in memo[n];
    :Return result;
    stop
  endif

endif

@enduml